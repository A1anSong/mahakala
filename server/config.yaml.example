db:
    config: sslmode=disable TimeZone=Asia/Shanghai
    default-db-name: mahakala
    host: 127.0.0.1
    initial-db-name: postgres
    password: postgres
    port: 5432
    user: postgres
exchanges:
    - api-key: 123456
      base-url: https://fapi.binance.com
      enabled: true
      name: binance_future
      secret-key: 123456
mahakala:
    analyze-amount: 500
    kline-interval: 30m
    max-update-routine: 100
    update-kline: true
